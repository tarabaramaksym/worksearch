	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Crawler",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\crawler\\crawl.js"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch API Tester",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\server\\src\\test.js"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch API Server",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\server\\src\\server.js"
		}
	]
}